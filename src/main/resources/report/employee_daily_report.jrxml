	<parameter name="employeeID" class="java.lang.Integer"/>
	<query language="SQL"><![CDATA[SELECT
    e.id,
    e.user_name,
    e.full_name,
    e.nic,
    e.email,
    e.address,
    e.dob,
    e.password,
    e.salary,
    e.type,
    e.role,
    e.admin_id,
    e.is_deleted,
    MAX(CASE WHEN ep.row_num = 1 THEN ep.phone END) AS phone1,
    MAX(CASE WHEN ep.row_num = 2 THEN ep.phone END) AS phone2,
    MAX(CASE WHEN ep.row_num = 3 THEN ep.phone END) AS phone3
FROM employee e
LEFT JOIN (
    SELECT
        phone,
        employee_id,
        ROW_NUMBER() OVER (PARTITION BY employee_id ORDER BY phone) AS row_num
    FROM employee_phone
    WHERE is_deleted = FALSE AND employee_id = $P{employeeID}
) ep ON e.id = ep.employee_id
WHERE e.is_deleted = FALSE AND e.id = $P{employeeID}
GROUP BY e.id;
]]></query>
