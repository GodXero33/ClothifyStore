<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="520.0" prefWidth="845.0" style="-fx-background-color: #000;"
            stylesheets="@../../../css/supplier-management.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.clothifystore.ecom.controller.form.menu.main.SupplierManagementFormController">
    <children>
        <AnchorPane prefHeight="520.0" prefWidth="227.0">
            <children>
                <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#addSupplierButtonOnAction" prefHeight="45.0" prefWidth="201.0" text="Add Supplier">
                    <cursor>
                        <Cursor fx:constant="HAND" />
                    </cursor>
                </Button>
                <Button alignment="CENTER" layoutX="13.0" layoutY="69.0" mnemonicParsing="false" onAction="#updateSupplierButtonOnAction" prefHeight="79.0" prefWidth="201.0" text="Update / Delete Supplier" textAlignment="CENTER" wrapText="true">
                    <cursor>
                        <Cursor fx:constant="HAND" />
                    </cursor>
                </Button>
            <Button layoutX="13.0" layoutY="159.0" mnemonicParsing="false" onAction="#allSuppliersButtonOnAction" prefHeight="45.0" prefWidth="201.0" text="All Suppliers">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
            </children>
        </AnchorPane>
        <AnchorPane fx:id="contentPane" layoutX="227.0" prefHeight="520.0" prefWidth="619.0" style="-fx-background-color: #121212;" />
    </children>
</AnchorPane>
